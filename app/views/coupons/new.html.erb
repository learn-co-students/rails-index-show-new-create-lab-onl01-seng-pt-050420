<%= form_tag coupons_path do %>
    <label>Coupon Form</label><br>
    
    <label>Coupon coupon_code:</label><br>
    <%= text_field_tag "coupon[coupon_code]" %><br>
    
    <label>Coupon store:</label><br>
    <%= text_field_tag "coupon[store]" %><br>

    <label>Coupon Description</label><br>

    <%= submit_tag "Submit Coupon" %>
<% end %>
